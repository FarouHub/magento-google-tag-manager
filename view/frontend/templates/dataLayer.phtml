<?php
/**
 * @var $block DK\GoogleTagManager\Block\DataLayer
 */
?>

<script>
    window.dataLayer = window.dataLayer || [];

    /** @type {Array} */
    let data = <?= /* @escapeNotVerified */ $block->getJsonDataLayers() ?>;

    if (data.length !== 0) {
        data.forEach((item) => {
            dataLayer.push(item);
        });

    }
</script>